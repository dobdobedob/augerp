	public EmpManagement calculateSalary(int salary){
		
		
		
		/*계산해야 할 것 
		 * 연봉 - 연봉은 입력해온 값을 가져온다
		 * 세전 급여 - 연봉 / 12 로 계산을 한다
		 * 세후 급여 - 세전에서 4대보험 제외한거
		 * 국민연금 - 4.5% / 9%
		 * 월급여가 553만원 이상이면 553만원으로 적용
		 * 월 소득이 35만원 이하면 35마원으로 적용
		 * 건강보험 - 3.495% / 6.99%
		 * 고용보험 - 0.9%
		 * 장기요양보험 - 건강보험료의 12.27% 반반 부담
		 */
		
		// 세전 급여 
		int sal_bt = salary /12;
		System.out.println(sal_bt + "세전 급여");
		
		//극민연금 
		double sal_kuk = 0;
		if(sal_bt <= 350000){
			sal_kuk = 350000 *0.045;
		}else if(sal_bt > 350000 && sal_bt < 5530000){
			sal_kuk  = sal_bt * 0.045;
		}else if(sal_bt >= 5530000){
			sal_kuk = 5530000 *0.045;
		}
		sal_kuk = Math.floor(sal_kuk/10)*10;
		//건강보험
		double sal_kun = sal_bt * 0.03495;
		sal_kun = Math.floor(sal_kun/10)*10;
		//장기요양보험
		double sal_san = sal_kun * 0.1227 * 0.5;
		sal_san = Math.floor(sal_san/10)*10;
		//고용보험 
		double sal_ko = sal_bt * 0.009;
		sal_ko = Math.floor(sal_ko/10)*10;
		//세후급여 
		double sal_price = sal_bt - sal_kuk - sal_kun - sal_san - sal_ko;
		EmpManagement calculateSalary = new EmpManagement();
		calculateSalary.setSal_bt(sal_bt);// 세전 급여 
		calculateSalary.setSal_kuk(sal_kuk);//극민연금 c
		calculateSalary.setSal_kun(sal_kun);//건강보험
		calculateSalary.setSal_san(sal_san);//장기요양보험
		calculateSalary.setSal_ko(sal_ko);//고용보험 
		calculateSalary.setSal_price(sal_price);//세후급여 
		
		System.out.println("paramMap::::   " + calculateSalary.getSal_kuk());
		return calculateSalary;
	}
	

	
